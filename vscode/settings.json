{
  "workbench.colorTheme": "Visual Studio Light",
  "editor.fontFamily": "'Fira Mono','Droid Sans Mono', 'Courier New', monospace, 'Droid Sans Fallback'",
  "window.menuBarVisibility": "toggle",
  "workbench.activityBar.visible": false,
  "workbench.statusBar.visible": true,
  "terminal.integrated.fontFamily": "'Fira Mono'",
  "search.exclude": {
    "**/node_modules": true,
    "**/target": true
  },
  "yaml.customTags": [
    "!And",
    "!If",
    "!Not sequence",
    "!Equals sequence",
    "!Or",
    "!FindInMap sequence",
    "!Base64",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!Select",
    "!Split",
    "!Join sequence"
  ],
  "shellcheck.customArgs": ["-x"],
  "yaml.format.enable": true,
  "window.zoomLevel": 0,
  "editor.minimap.enabled": true,
  "explorer.openEditors.visible": 0,
  "[markdown]": {
    "editor.tabSize": 2
  },
  // "markdownlint.config": {
  //   "MD029": false,
  //   "MD007": false
  // },
  "editor.smoothScrolling": true,
  "workbench.editor.enablePreview": false,
  "extensions.ignoreRecommendations": false,
  "python.linting.pylintPath": "pylint",
  "python.linting.pycodestyleEnabled": true,
  "python.linting.flake8Enabled": true,
  "python.linting.mypyEnabled": true,
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "cSpell.spellCheckDelayMs": 3000,
  "cSpell.userWords": [
    "abschliessen",
    "anforderungsgemäss",
    "anschliessend",
    "apéro",
    "architecting",
    "ausschliesslich",
    "aussen",
    "begrüssung",
    "burndown",
    "cacheable",
    "cloudformation",
    "cmii",
    "cognito",
    "crowdlending",
    "cynefin",
    "diskontierung",
    "diskontierungssatz",
    "ergon",
    "fertigkeitsgrad",
    "fogcreek",
    "fortschrittsmass",
    "functionboxing",
    "füsse",
    "gemerged",
    "gemäss",
    "giga",
    "gleichmässig",
    "gleichmässiges",
    "granulare",
    "grobgranulare",
    "grosse",
    "grossem",
    "grossen",
    "grossprojekt",
    "grösse",
    "grösser",
    "grössere",
    "grösseres",
    "grösste",
    "heisse",
    "heisst",
    "innominatvertrag",
