    "klassifizierungslehre",
    "kommunikationsmassnahme",
    "kontrollmassnahmen",
    "korrekturmassnahmen",
    "lenkungsausschuss",
    "lifecycle",
    "massgebend",
    "massnahme",
    "massnahmen",
    "mergen",
    "microservice",
    "mittelgrosse",
    "mqtt",
    "nfrs",
    "nutzniesser",
    "nygard",
    "ordnungsmässigen",
    "otobos",
    "owasp",
    "papeterie",
    "pdftk",
    "phasenübergänge",
    "pojo",
    "pptx",
    "preferenzen",
    "prerender",
    "projektgrösse",
    "pönale",
    "qualitätsmassnahmen",
    "rdbms",
    "reduktionsalgorithmen",
    "refactorings",
    "regelmässig",
